{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "quasar",
      "camelcase": "Quasar",
      "title": "Quasar",
      "scope": "source.quasar",
      "file-types": [
        "quasar"
      ],
      "injection-regex": "^quasar$",
      "class-name": "TreeSitterQuasar"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "A tree-sitter grammar for the Quasar programming language",
    "authors": [
      {
        "name": "Graikos Parmenion",
        "email": "par.graikos@icloud.com"
      }
    ],
    "links": {
      "repository": "https://github.com/parmeniong/tree-sitter-quasar"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}